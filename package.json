{
  "name": "daanarx-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "npm run clean:native && expo start --clear",
    "start:dev": "expo start --dev-client --clear",
    "prebuild": "npx expo prebuild --clean",
    "build:ios": "npx expo run:ios",
    "build:android": "npx expo run:android",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "web": "expo start --web",
    "tunnel": "expo start --tunnel",
    "clean": "rm -rf node_modules ios android .expo && npm install",
    "clean:native": "rm -rf ios android .expo"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
